<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.ProfileFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/profileuser"
            android:id="@+id/profile_user"/>


       <Button
           android:layout_marginTop="10dp"
           android:layout_gravity="center_horizontal"
           android:text="Admin Login"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:id="@+id/add_text_pfofile"/>


        <Button
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal"
            android:text="Mountain Data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/add_text_mountain"/>

        <Button
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal"
            android:text="Beach Data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/add_text_beach"/>

        <Button
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal"
            android:text="Lake Data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/add_text_lake"/>

        <Button
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal"
            android:text="Forest Data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/add_text_forest"/>


    </LinearLayout>


</FrameLayout>